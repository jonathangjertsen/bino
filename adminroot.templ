package main

import (
    "github.com/jonathangjertsen/bino/ln"
)

templ AdminRootPage(data *CommonData) {
	@Layout(data) {
        <h1>{data.Ln(ln.AdminRoot)}</h1>
        <div class="card b-card">
            <ul>
                <li>@NavbarPageLink(data.Ln(ln.AdminManageSpecies), "/species")</li>
                <li>@NavbarPageLink(data.Ln(ln.AdminManageTags), "/tag")</li>
                <li>@NavbarPageLink(data.Ln(ln.AdminManageHomes), "/homes")</li>
            </ul>
        </div>
    }
}
