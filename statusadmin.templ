package main

import (
    "github.com/jonathangjertsen/bino/ln"
)

type StatusLangs struct {
    ID int32
    Names
}

templ StatusPage(data *CommonData, statuses []StatusLangs) {
    @CRUDPage{
        Header: ln.ManageStatuses,
    }.Component(data) {
        for _, status := range statuses {
            @UpdateRow(data, status.ID, status) {}
        }
        @CreateRow(data) {}
    }
    <script src="/static/statusadmin.js"></script>
}
