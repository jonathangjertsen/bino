{
    "HTTP": {
        "URL": ":8080",
        "ReadTimeoutSeconds": 10,
        "ReadHeaderTimeoutSeconds": 10,
        "WriteTimeoutSeconds": 15,
        "IdleTimeoutSeconds": 60,
        "StaticDir": "cmd/static"
    },
    "Auth": {
        "SessionKeyLocation": "secret/session_key",
        "OAuthCredentialsLocation": "secret/oauth.json",
        "ClientID": "<your-client-id>.apps.googleusercontent.com",
        "OAuthRedirectURI": "<base-url>./oauth2/callback"
    },
    "Privacy": {
        "LogDeletionPolicy": 3,
        "RevokeConsentPolicy": 14
    },
    "GoogleDrive": {
        "ServiceAccountKeyLocation": "secret/serviceaccount.json",
        "DriveBase": "<ID>",
        "JournalFolder": "<ID>",
        "TemplateFile": "<ID>",
        "ExtraJournalFolders": [
        ]
    },
    "DB": {
        "URL": "localhost",
        "Port": "5432",
        "CacheFile": "cache.db"
    },
    "SystemLanguage": 1,
    "SystemBaseURL": "bino.<your-domain>"
}
